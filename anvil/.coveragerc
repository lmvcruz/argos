[run]
omit =
    anvil/__main__.py
    anvil/cli/*.py
    anvil/cli/__init__.py
    anvil/cli/main.py
    anvil/cli/commands.py
    */anvil/cli/*
    */tests/*
    */__pycache__/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
